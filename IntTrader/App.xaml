<Application x:Class="IntTrader.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="clr-namespace:IntTrader.ViewModel"
             StartupUri="View\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            
            <local:MainViewModel x:Key="MainViewModel"></local:MainViewModel>
            
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/BasicResources.xaml"></ResourceDictionary>
                <!--<ResourceDictionary Source="Resources/ScrollViewer.xaml"></ResourceDictionary>-->
                <ResourceDictionary Source="Resources/BalanceResources.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/OrderBookResources.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Themes/ExpressionDark.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
